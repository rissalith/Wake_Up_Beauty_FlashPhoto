# 后台管理 API 服务配置
# 复制此文件为 admin-api.env 并填入实际值

# 服务配置
NODE_ENV=production
PORT=3000

# JWT 配置
JWT_SECRET=your_jwt_secret

# 数据库配置
SHARED_DB_PATH=/app/data/flashphoto.db

# 腾讯云 COS 配置
COS_SECRET_ID=your_cos_secret_id
COS_SECRET_KEY=your_cos_secret_key

# 微信配置
WX_APPID=your_appid
WX_APPSECRET=your_secret
